<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="20.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.humancreator.controller.EditFormController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <BorderPane prefHeight="384.0" prefWidth="529.0">
      <bottom>
         <ButtonBar prefHeight="40.0" prefWidth="111.0" BorderPane.alignment="CENTER">
           <buttons>
             <Button fx:id="saveButton" minWidth="59.0" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="25.0" prefWidth="123.0" text="Save" />
             <Button fx:id="cancelButton" layoutX="277.0" layoutY="18.0" mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="25.0" text="Cancel" />
           </buttons>
         </ButtonBar>
      </bottom>
      <center>
         <Pane prefHeight="128.0" prefWidth="152.0" BorderPane.alignment="CENTER">
            <children>
               <TextField fx:id="name" prefHeight="25.0" prefWidth="180.0" />
               <TextField fx:id="age" layoutY="43.0" prefHeight="25.0" prefWidth="180.0" />
               <TextField fx:id="birthday" layoutY="82.0" prefHeight="25.0" prefWidth="180.0" promptText="yyyy-MM-dd" />
            </children>
         </Pane>
      </center>
   </BorderPane>
</VBox>
